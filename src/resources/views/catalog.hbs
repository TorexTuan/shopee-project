<script>
    const $ = document.querySelector.bind(document)
    const $$ = document.querySelectorAll.bind(document)

    const swiperList = [
        {
            mainSelector: '.swiper-mall',
            nextSlideBtn: '.button-next-mall',
            prevSlideBtn: '.button-prev-mall',
            slidesPerView: 6,
            translate3d: 1200,
        }
    ]

    for(const swiperItem of swiperList) {

        const nextSlideBtn = $(swiperItem.nextSlideBtn)
        const prevSlideBtn = $(swiperItem.prevSlideBtn)
        const mainSwiper = $(swiperItem.mainSelector)
        const swiperWrapper = $(swiperItem.mainSelector + ' .swiper-wrapper')

        //Customize swiper slider for each component
        var swiper = new Swiper(swiperItem.mainSelector, {
            slidesPerView: swiperItem.slidesPerView,
            noSwiping: true,
            noSwipingClass: '.swiper-slide',
            speed: 500,
            navigation: {
                nextEl: swiperItem.nextSlideBtn,
                prevEl: swiperItem.prevSlideBtn
            },
        })

        const slidesLength = swiper.slides.length
        const slideWidth = $(swiperItem.mainSelector + ' .swiper-slide').offsetWidth
        const swiperSlideQuantity = swiper.slides.length
        const swiperViewWidth = swiper.width
        
        //Handle transform with next btn
        let translate3dIncrement = 0
        nextSlideBtn.onclick = function() {
            swiperWrapper.style.transform = `translate3d(${(translate3dIncrement - swiperItem.translate3d)}px, 0px, 0px)`
            translate3dIncrement = translate3dIncrement - swiperItem.translate3d

            let totalWidthSwiper = slideWidth * swiperSlideQuantity
            let lastView = totalWidthSwiper - Math.abs(translate3dIncrement)

            prevSlideBtn.style.display = 'flex'
            if(lastView === swiperViewWidth) {
                this.style.display = 'none'
            }
        }

        //Handle transform with prev btn
        prevSlideBtn.onclick = function() {
            swiperWrapper.style.transform = `translate3d(${(translate3dIncrement + swiperItem.translate3d)}px, 0px, 0px)`
            translate3dIncrement = translate3dIncrement + swiperItem.translate3d

            nextSlideBtn.style.display = 'flex'
            if(translate3dIncrement === 0) {
                this.style.display = 'none'
            }
        }
        //Handle btn affect
        const array = [mainSwiper, nextSlideBtn, prevSlideBtn]
        array.forEach(item => item.onmouseover = function() {
            nextSlideBtn.classList.add('keyFrames-open')
            prevSlideBtn.classList.add('keyFrames-open')
        })

        array.forEach(item => item.onmouseout = function() {
            nextSlideBtn.classList.remove('keyFrames-open')
            prevSlideBtn.classList.remove('keyFrames-open')
        })
    }
        

    //  Expand list
    const watchMore = $$(".list-watch-more")
    const watchMoreArr = Array.from(watchMore)
    
    for(const watch of watchMore) {
        watch.onclick = function() {
            this.style.display = 'none'
        } 
    }

    //  Heart
    const hearts = $$('.products__content__sell__heart')
    const likedHearts= $$('.products__content__sell__heart--liked')
    const unlikedHearts = $$('.products__content__sell__heart--unliked')

    const likedHeartsArr = Array.from(likedHearts)
    const unlikedHeartsArr = Array.from(unlikedHearts)
    
    function checkChild(heartIndex) {
        const getUnlikedHeart = unlikedHeartsArr.find((unlikeHeart, index) => {
            return index === heartIndex
        })

        const getLikedHeart = likedHeartsArr.find((likedHeart, index) => {
            return index === heartIndex
        })

        checkHeartDisplay(getUnlikedHeart, getLikedHeart) 
    }

    function checkHeartDisplay(getUnlikedHeart, getLikedHeart) {
        if(getLikedHeart.style.display == 'none') {
            getLikedHeart.style.display = 'block'
            getUnlikedHeart.style.display = 'none'
        }
        else {
            getLikedHeart.style.display = 'none'
            getUnlikedHeart.style.display = 'block'
        }
    }

    hearts.forEach((heart, index) => {
        heart.onclick = (event) => {
            checkChild(index)
            event.preventDefault()
            }
        })

    // khac 
    const different = $('.filter__list__differ__text')
    const tickTable = $('.diff__places__table')

    different.onclick = function(event) {
        event.stopPropagation()
        if(!tickTable.matches('.diff__places__table--zoomIn')) {
            tickTable.classList.add('diff__places__table--zoomIn')
            tickTable.classList.remove('diff__places__table--zoomOut')
            tickTable.style.display = 'block'
        }
        else {
            tickTable.classList.remove('diff__places__table--zoomIn')
            tickTable.classList.add('diff__places__table--zoomOut')
            setTimeout( function() {
                tickTable.style.display = 'none'
            },100)
        }
    }

    window.onclick = () => {
        tickTable.classList.add('diff__places__table--zoomOut')
        tickTable.classList.remove('diff__places__table--zoomIn')
        inputLeftWrapper.style.outline = 'unset'
        listWrapper.style.display = 'block'
        setTimeout( function() {
            tickTable.style.display = 'none'
        },100)
        
    }

    tickTable.addEventListener('click', (event) => {
        event.stopPropagation()
    })

    // khác - table - tick
    const checkboxes = $$('.checkbox-table-diff')
    const resetBtn = $('.diff__footer__reset-btn')
    const confirmBtn = $('.diff__footer__confirm-btn')
    const checkLabel = $$('.diff__tick__list__item')

    const checkboxArray = Array.from(checkboxes)

    checkboxes.forEach(check => {
        check.onclick = function ()  {
            const checked = checkboxArray.some(checkbox => {
                return checkbox.checked
            })
            confirmCheck(checked)
        }
    })

    function confirmCheck(checked) {
        if(checked){
                resetBtn.disabled = false
                resetBtn.classList.add('active-filter-places')

                confirmBtn.disabled = false 
                confirmBtn.classList.add('active-filter-places')
            }else {
                resetBtn.disabled = true
                resetBtn.classList.remove('active-filter-places')

                confirmBtn.disabled = true 
                confirmBtn.classList.remove('active-filter-places')
            }
    }
    
    // diff__alphabet
    const alphabets = $$('.diff__body__alphabet')
    
    alphabets.forEach(alphabet => {
        alphabet.onclick = function () {
            document.querySelector('.diff__body__alphabet.active__diff__alphabet').classList.remove('active__diff__alphabet')
            this.classList.add('active__diff__alphabet')
        }
    })
    

   

</script>